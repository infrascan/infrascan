{
  "name": "@infrascan/aws",
  "version": "0.4.1",
  "description": "A convenience package to register all AWS Scanners with an instance of the Infrascan SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist ; tsup --config internal/tsconfig/tsup.json",
    "prepublish": "rm -rf dist ; tsup --config internal/tsconfig/tsup.json",
    "test": "echo \"Error: no test specified\"",
    "lint": "tsc --noEmit ; prettier src/**/*.ts -c ; eslint .",
    "lint:fix": "prettier src/**/*.ts -w ; eslint . --fix",
    "clean": "rm -rf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infrascan/infrascan.git"
  },
  "keywords": [
    "AWS",
    "Cloud Infrastructure",
    "Infrastructure Diagram",
    "Scanner"
  ],
  "author": "Liam Farrelly <liam@l3f7.dev> (https://whoisli.am/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/infrascan/infrascan/issues"
  },
  "homepage": "https://github.com/infrascan/infrascan#readme",
  "devDependencies": {
    "@infrascan/sdk": "^0.4.0",
    "@infrascan/tsconfig": "*"
  },
  "dependencies": {
    "@infrascan/aws-api-gateway-scanner": "^0.3.0",
    "@infrascan/aws-autoscaling-scanner": "^0.2.5",
    "@infrascan/aws-cloudfront-scanner": "^0.3.0",
    "@infrascan/aws-cloudwatch-logs-scanner": "^0.3.0",
    "@infrascan/aws-dynamodb-scanner": "^0.3.0",
    "@infrascan/aws-ec2-scanner": "^0.3.0",
    "@infrascan/aws-ecs-scanner": "^0.3.0",
    "@infrascan/aws-elastic-load-balancing-scanner": "^0.3.0",
    "@infrascan/aws-kinesis-scanner": "^0.2.0",
    "@infrascan/aws-lambda-scanner": "^0.3.0",
    "@infrascan/aws-rds-scanner": "^0.3.0",
    "@infrascan/aws-route53-scanner": "^0.3.0",
    "@infrascan/aws-s3-scanner": "^0.3.0",
    "@infrascan/aws-sns-scanner": "^0.3.0",
    "@infrascan/aws-sqs-scanner": "^0.3.0",
    "@infrascan/aws-step-function-scanner": "^0.2.0"
  }
}
