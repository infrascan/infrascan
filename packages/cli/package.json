{
  "name": "@infrascan/cli",
  "version": "0.7.0",
  "description": "Thin command line wrapper around the infrascan sdk",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "rm -rf dist ; tsup --config internal/tsconfig/app.tsup.json ; cp -r ./src/templates ./dist/templates",
    "lint": "tsc --noEmit ; eslint .",
    "clean": "rm -rf dist node_modules"
  },
  "bin": {
    "infrascan": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "AWS",
    "Infrastructure Diagram",
    "Infrastructure Graph",
    "Cloud Infrastructure",
    "CLI"
  ],
  "author": "Liam Farrelly <liam@l3f7.dev> (https://whoisli.am/)",
  "license": "MPL-2.0",
  "dependencies": {
    "@aws-sdk/credential-providers": "3.624.0",
    "@infrascan/aws": "^0.5.4",
    "@infrascan/cytoscape-serializer": "^0.3.4",
    "@infrascan/fs-connector": "^0.3.0",
    "@infrascan/informational-serializer": "^0.2.0",
    "@infrascan/sdk": "^0.6.0",
    "@rushstack/ts-command-line": "^4.15.2",
    "@smithy/shared-ini-file-loader": "^3.1.12",
    "ejs": "^3.1.9",
    "minimatch": "^6.1.6",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.609.0",
    "@infrascan/shared-types": "^0.7.0",
    "@smithy/types": "^3.3.0",
    "@types/ejs": "^3.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  }
}
