{
  "name": "@infrascan/cli",
  "version": "0.2.1",
  "description": "Thin command line wrapper around the infrascan sdk",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "rm -rf dist ; tsup",
    "lint": "tsc --noEmit ; eslint .",
    "clean": "rm -rf dist node_modules"
  },
  "tsup": {
    "clean": true,
    "splitting": false,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "target": "node18"
  },
  "bin": {
    "infrascan": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "AWS",
    "Infrastructure Diagram",
    "Infrastructure Graph",
    "Cloud Infrastructure",
    "CLI"
  ],
  "author": "Liam Farrelly <liam@l3f7.dev> (https://whoisli.am/)",
  "license": "MPL-2.0",
  "dependencies": {
    "@aws-sdk/credential-providers": "3.428.0",
    "@infrascan/aws": "^0.2.0",
    "@infrascan/fs-connector": "^0.2.0",
    "@infrascan/sdk": "^0.2.0",
    "@rushstack/ts-command-line": "^4.15.2",
    "minimatch": "^6.1.6"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.428.0",
    "@infrascan/shared-types": "^0.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
