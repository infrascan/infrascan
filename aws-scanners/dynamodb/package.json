  "devDependencies": {
    "@infrascan/aws-codegen": "^0.1.0",
    "@infrascan/shared-types": "^0.1.0",
    "eslint-config-codegen": "^1.0.0",
    "tsconfig": "^0.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
