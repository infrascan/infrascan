{
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "apps/*",
    "aws-scanners/*",
    "internal/*",
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.2.0",
    "@mxssfd/typedoc-theme": "^1.1.2",
    "@turbo/gen": "^1.10.13",
    "eslint": "^8.57.0",
    "tap": "^18.7.0",
    "tsup": "^8.0.0",
    "turbo": "^1.9.9",
    "typedoc": "^0.25.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^7.8.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "codegen": "turbo run codegen",
    "fmt": "turbo run fmt",
    "lint": "turbo run lint",
    "lint:all": "eslint .",
    "test": "turbo run test",
    "docs": "typedoc --exclude internal/tsconfig --exclude internal/eslint-config-custom --exclude internal/eslint-config-codegen --exclude internal/codegen ; cp -r assets/* docs/assets",
    "clean": "turbo run clean ; rm -rf node_modules",
    "release": "turbo run test ; npx changeset publish"
  }
}
